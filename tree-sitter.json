{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "plain",
      "camelcase": "Plain",
      "title": "Plain",
      "scope": "source.plain",
      "file-types": [
        "txt",
        "md"
      ],
      "injection-regex": "^plain$",
      "class-name": "TreeSitterPlain"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "plaintext or unknown files",
    "authors": [
      {
        "name": "Jacob Rothstein",
        "email": "hi@jbr.me",
        "url": "https://jbr.me/"
      }
    ],
    "links": {
      "repository": "https://github.com/jbr/tree-sitter-plain"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}